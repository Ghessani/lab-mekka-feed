generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model feedback {
  id        Int      @id @default(autoincrement())
  email     String? 
  phone     String?
  message   String?
  Language  LanguageType
  overallRating reactionType
  recp     reactionType?
  emrg     reactionType?
  bill     reactionType?
  cash     reactionType?
  care     reactionType?
  labo     reactionType?
  oprt     reactionType?
  radl     reactionType?
  createdAt DateTime @default(now())
}
 
 
 

 
enum reactionType {
  ABSOLUTELY_DISATISFIED
  DISATISFIED
  OK
  SATISFIED
  EXTREMELY_SATISFIED
}

enum LanguageType {
  EN
  FR
  AR
}

